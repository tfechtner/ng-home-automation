{
  "name": "nest-home-frontend",
  "version": "0.1.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tfechtner/ng-home-automation.git"
  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start:dev": "ng serve --port 4100",
    "build": "ng build",
    "build:prod": "ng build --prod --configuration production",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint ngx-admin-demo --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "prepush": "npm run lint",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points",
    "deploy": "pm2 deploy ../ecosystem.json prod-frontend"
  },
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@nebular/eva-icons": "^7.0.2",
    "@nebular/theme": "^7.0.2",
    "@ngxs/devtools-plugin": "^3.7.2",
    "@ngxs/store": "^3.7.2",
    "@types/socket.io-client": "^1.4.36",
    "bootstrap": "4.3.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "eva-icons": "^1.1.3",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "nebular-icons": "1.1.0",
    "normalize.css": "6.0.0",
    "roboto-fontface": "0.8.0",
    "rxjs": "6.5.5",
    "rxjs-compat": "6.3.0",
    "socicon": "3.0.5",
    "socket.io-client": "^2.4.0",
    "style-loader": "^1.3.0",
    "tslib": "^2.0.3",
    "typeface-exo": "0.0.22",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular/cli": "^11.2.15",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "11.2.14",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@types/node": "^12.20.13",
    "codelyzer": "^6.0.2",
    "husky": "0.13.3",
    "npm-run-all": "4.0.2",
    "rimraf": "2.6.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "3.2.2",
    "tslint": "^5.20.1",
    "typescript": "4.0.8"
  }
}
